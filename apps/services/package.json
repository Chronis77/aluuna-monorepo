{
  "name": "aluuna-services",
  "version": "2.0.0",
  "description": "Aluuna Services - Modern TypeScript AI-first therapy server with Bun",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "build": "bun build src/index.ts --outfile dist/index.js --target node",
    "start": "bun run dist/index.js",
    "clean": "rm -rf dist",
    "postinstall": "bunx prisma generate && bun run build",
    "migrate:export": "bun run scripts/migrate-to-railway.ts export",
    "migrate:help": "bun run scripts/migrate-to-railway.ts help"
  },
  "dependencies": {
    "@google-cloud/text-to-speech": "^5.0.0",
    "@prisma/client": "^5.10.0",
    "@trpc/client": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jose": "^6.0.12",
    "jsonwebtoken": "^9.0.2",
    "openai": "^4.20.1",
    "pg": "^8.11.3",
    "redis": "^4.6.0",
    "socket.io": "^4.7.4",
    "winston": "^3.11.0",
    "zod": "^3.22.0",
    "zod-prisma-types": "^2.8.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.0.0",
    "bun-types": "latest",
    "prisma": "^5.10.0"
  },
  "keywords": [
    "therapy",
    "ai",
    "typescript",
    "bun",
    "trpc",
    "openai",
    "aluuna"
  ],
  "author": "Aluuna Team",
  "license": "MIT"
}
